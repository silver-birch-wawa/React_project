	 .body{
	  font-family:'Comic Sans MS', cursive, sans-serif;
	  padding: 0px;
	  background: #f1f1f1;
      height: 100%;
	 }
	  /* 导航栏 */
	  .header{
	  	font-family:'Comic Sans MS', cursive, sans-serif;
        background: white;
        /*#b0d1e3 浅蓝*/
        align-items: center;
        justify-content: space-between;
        display:flex !important;
        padding:0 1rem;
        height:3rem;
	  }
	 .html nav>ul{list-style: none;align-items: center;display:flex;margin-top: 1rem;}
	 .html nav>ul>li{padding:1rem;}
	 .html nav>ul a{text-decoration: none;color:black;align-items:center;display:flex;}

    /* 主页 */
    container{
	  display:flex; 
      margin-top:20px;
      height:100%;
      font-family: 'Comic Sans MS', cursive, sans-serif;
	  }
	section{
        height:1.5rem;
	  	flex-direction: column;
        background-color: #b0d1e3;
        border-radius: 5px;
	  }

    .catalog{
      text-align:left;

      height:35px;
      font-size:20px;
      /*color: #337ab7;
      color: #94f0f0;*/
      color: grey;
      border-radius: 5px;
      margin-top: 10px;
      background-color: white;

    }

    #display-box{
      /*border: 1px solid grey;*/
      padding: 10px;
    }
    

    /*表格*/
    #customers
      {

	      font-family:'Comic Sans MS', cursive, sans-serif;
	      width:100%;
	      text-align:center;
	      border-collapse:collapse;
	      background-color:white;       
      }
    #customers td, #customers th 
      {
	      font-size:0.6rem;
	      /*border:1px solid #98bf21; */
	      border-bottom:1px solid #e8e8e8;;
	      padding:8px 15px 8px 15px;     
      }
    .ant-collapse > .ant-collapse-item > .ant-collapse-header{
    	line-height: 4px !important;
    }
    td >.ant-collapse > .ant-collapse-item > .ant-collapse-header .arrow{
    	display:none !important;
    }
    #customers th 
      {
	      font-size:0.7rem;
	      padding-top:15px;
	      padding-bottom:15px;
	      /*background-color:#A7C942;*/
	      background: #fafafa;
	      /*color:#ffffff;*/
      }
    #customers tr.alt td 
      {
	      font-size:0.66rem;
	      color:#000000;
	      z-index:5;
	      background-color:#aba4a4 !important;
      }
    #customers .td-select{
    	/*padding:0px 0px 0px 0px!important;*/
